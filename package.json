{
  "name": "parking-watcher",
  "version": "1.0.0",
  "description": "Monitor parking availability and notify via Telegram",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon --watch index.js --ignore data/* --ignore node_modules --ext js,mjs",
    "test": "echo \"No tests\" && exit 0",
    "dev:test": "NODE_ENV=test node -r dotenv/config index.js dotenv_config_path=.env.test",
    "dev:test:watch": "NODE_ENV=test nodemon -r dotenv/config --watch index.js --ignore data/* --ignore node_modules --ext js,mjs index.js dotenv_config_path=.env.test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@cspell/dict-ru_ru": "^2.3.2",
    "@eslint/js": "^9.37.0",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "nodemon": "^3.1.0"
  }
}
